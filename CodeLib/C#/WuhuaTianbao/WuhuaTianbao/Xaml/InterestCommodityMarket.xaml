        </Grid>
        <Grid Grid.Column="2" Grid.Row="0" HorizontalAlignment="Stretch" Name="grid21" VerticalAlignment="Stretch">
            <Grid.RowDefinitions>
                <RowDefinition Height="50*" />
                <RowDefinition Height="50*" />
            </Grid.RowDefinitions>
